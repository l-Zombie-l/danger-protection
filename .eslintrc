{
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
        "@typescript-eslint",
        "prettier",
        "import"
    ],
    "extends": [
        "plugin:@typescript-eslint/recommended",
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module"
      },
      "env": {
        "es6": true,
        "browser": true,
        "node": true
      },
      "rules": {
        "no-debugger": "off",
        "no-console": 0,
        "@typescript-eslint/explicit-module-boundary-types": "off"
      }
  }